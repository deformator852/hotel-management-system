{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "single_quote": true,
        "strict_comparison": true,
        "ordered_class_elements": {
            "order": [
                "use_trait",
                "case",
                "constant_public",
                "constant_protected",
                "constant_private",
                "property_public",
                "property_protected",
                "property_private",
                "construct",
                "destruct",
                "method:__invoke",
                "method_public_static",
                "method_public",
                "method_protected_static",
                "method_protected",
                "method_private_static",
                "method_private",
                "magic"
            ]
        }
    }
}
